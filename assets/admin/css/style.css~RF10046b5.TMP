/*----------------- generals end -------------------*/

.wrapper{
	width:980px;
	overflow:hidden;
	margin:0 auto;
	padding:10px;
	background:#fff;
}
.header{
	height:100px;
	/*background:#0d5b88;*/
	background:#017193;
}
h1{
	font-size:30px;
	line-height:100px;
	color:#fff;
	text-align:center;
}
.login_panel{
	width:400px;
	
	overflow:hidden;
	border:1px solid #ddd;
	margin:0 auto;
	margin-top:30px;
	margin-bottom:30px;
}
.login_panel fieldset{
	padding:20px;
	background:#eaeaea;
	border:3px solid #fff;
}
.login_panel p{
	padding:5px 0;
	overflow:hidden;
}
.login_panel label{
	width:130px;
	display:block;
	float:left;
	font-size:14px;
	line-height:30px;
	height:30px;
}

.login_panel input[type=text],.login_panel input[type=password], .login_panel select{
	width:200px;
	padding:0 5px;
	line-height:30px;
	height:30px;
	border:1px solid #ddd;
	float:left;
}
.login_panel select{
	width:212px;
	padding:5px;
	line-height:20px;
	height:32px;
}
.login_panel input[type=submit], .login_panel input[type=reset]{
	padding:5px 10px;
	border:1px solid #ddd;
	background:#333;
	color:#fff;
}

/*------------mainleftpanel--------------*/
.mainleftpanel{
	width:200px;
	overflow:hidden;
	padding-top:10px;
	padding-bottom:5px;
}

.mainrigthpanel{
	width:757px;
	margin-top:10px;
	overflow:hidden;
	padding:10px;
	padding-bottom:0;
	padding-right:0;
	border:1px solid #ddd;
}
.mainrigthpanel .box{
	width:128px;
	height:128px;
	border:#ddd 1px solid;
	float:left;
	margin:0 10px 10px 0;
}

.nav{
}
.nav li{
	display:block;
	margin:0 0 5px 0;
}
.nav li a{
	/*display:block;
	line-height:15px;
	padding:5px;
	color:#333;
