}


/*---------------agencyapplication-----------------*/
.agencyapplication{
	overflow:hidden;
	padding:20px;
	background:#EAFAFF;
	margin:10px auto;
	border:1px solid #D4EFF7;
}
.agencyapplication fieldset{
	padding-top:10px;
	border-top:#bddee8 solid 1px;
	margin-top:20px;
}
.agencyapplication fieldset legend{
	color:#0D5B88;
	font-size:16px;
	font-weight:bold;
	line-height:18px;
	padding-right:2px;
	text-transform:capitalize;
	padding-bottom:5px;
	font-family:"Times New Roman", Times, serif;
}
.agencyapplication fieldset p{
	display:block;
	overflow:hidden;
	padding:6px 0;
}
.agencyapplication h2{
	display:block;
	font-family:"Times New Roman", Times, serif;
	font-size:20px;
	line-height:22px;
	color:#333;
	font-weight:normal;
}
.agencyapplication h2 span{
	display:block;
	font-size:14px;
	line-height:16px;
	color:#F60;
	background-color:inherit;
	padding-top:5px;
}
.agencyapplication label{
	width:350px;
	display:block;
	float:left;
	text-align:right;
	padding-right:20px;
	font-size:13px;
	line-height:15px;
	padding:4px 15px 4px 0;
}
.red{
	color:#F00;
	background-color:inherit;
}
.textinput{
	background:url(<PATH_IMG>text_inp_bg.jpg) no-repeat 0 0;
	width:240px;
	height:22px;
	line-height:22px;
	color:#333;
	padding:0 5px;
	font-size:12px;
	float:left;
}
.submitbtn{
	height:23px;
	line-height:15px;
	font-size:13px;
	padding:0 15px;
	border:1px solid #4e4d4d;
	background:url(<PATH_IMG>submitbtnbg.jpg) repeat-x 0 0 #2b2b2b;
	color:#fff;
}
.login_paneladd{
	width:93%;
	display:block;
	border:none;
}
.login_paneladd h2{
	margin-bottom:15px;
	display:block;
}
.login_paneladd input[type=text]{
	border:1px solid #999;
	width:300px;
}
.login_paneladd textarea{
	border:1px solid #999;
	width:300px;
}




